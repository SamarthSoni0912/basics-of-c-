#include<iostream>
using namespace std;
int main(){
    int n;
    cout<<"enter n="<<endl;
    cin>>n;
    int i=1; 
    while(i<=n){
        char k=65+n-i;
        char l=k;
        int j=1;
        while(j<=i){
            cout<<l;
            l++;
            j++;
        }
        cout<<endl;
        i++;
    }
}
